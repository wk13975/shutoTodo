	width: 100%;
	min-height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}

/* 我写的 开始 */
.title-line {
	height: 90rpx;
	line-height: 90rpx;
	padding: 0 20rpx 0 20rpx;
	margin: 5rpx 10rpx 5rpx 10rpx;
	font-size: 36rpx;
	font-weight: 500;
	/* background-color: #ffd69b; */
	color: #383838;
}
.info-box {
	padding: 10rpx 20rpx 10rpx 20rpx;
}
.info-line {
	min-height: 80rpx;
	min-line-height: 80rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	/* background-color: #09BB07; */
	padding-left: 10rpx;
	margin-top: 5rpx;
	margin-bottom: 5rpx;
}
.leftTitle {
	line-height: 80rpx;
	min-width: 140rpx;
	margin-right: 20rpx;
	color: #434343;
}
.rightContent {
	-webkit-flex: 1;
	-webkit-box-flex: 1;
	        flex: 1;
	border-bottom: 1rpx #F4F5F6 solid;
	/* background-color: #ffffff; */
	padding: 5rpx 20rpx 5rpx 0;
}
.rightContent>text {
	line-height: 80rpx;
}
picker, .rightContent>input {
	height: 70rpx;
	line-height: 80rpx;
	padding-left: 10rpx;
	padding-right: 10rpx;
	border-radius: 10rpx;
	border: 1rpx solid #d3d3d3;
	background-color: #FFFFFF;
}
.pickerview {
	height: 70rpx;
	line-height: 70rpx;
}
.rightContent>.uni-textarea {
	min-height: 120rpx;
	border-radius: 10rpx;
	border: 1rpx solid #d3d3d3;
	background-color: #ffffff;
}
textarea{
	min-height: 120rpx;
	padding-left: 10rpx;
	padding-right: 10rpx;
	border-radius: 10rpx;
}
._select {height: 70rpx;/* font-size: 30upx; */margin: 5rpx 50rpx 5rpx 0;border-radius: 10rpx;}
.uni-media-list {
	padding: 10rpx 30rpx 10rpx;
	margin-bottom: 10rpx;
	background-color: #FFFFFF;
}
.uni-media-list-body>view{
	line-height: 45rpx;
	font-size: 30rpx;
}
.workInfo {
	height: 80%;
	margin-top: 94rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	display: block;
}
/* 我写的 结束 */
.tabs {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	overflow: hidden;

	
	height: 100vh;
	
}
.scroll-h {
	width: 750rpx;
	height: 80rpx;

	-webkit-box-orient: horizontal;

	-webkit-box-direction: normal;

	-webkit-flex-direction: row;

	        flex-direction: row;

	
	white-space: nowrap;
	
	/* flex-wrap: nowrap; */
	/* border-color: #cccccc;
	border-bottom-style: solid;
	border-bottom-width: 1px; */
}
.line-h {
	height: 1rpx;
	background-color: #cccccc;
}
.uni-tab-item {
	
	display: inline-block;
	
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	text-align: center;
	-webkit-flex: 1;
	-webkit-box-flex: 1;
	        flex: 1;
}
.uni-tab-item-title {
	color: #555;
	font-size: 30rpx;
	height: 80rpx;
	line-height: 80rpx;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	
	white-space: nowrap;
	
}
.uni-tab-item-title-active {
	color: #007AFF;
}
.swiper-box {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	background-color: #f1f1f1;
	height: 100%;
	
	height: 100vh;
	
}
.myswiper-item {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	
	height: 100vh;
	
}
.scroll-v {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	
	width: 750rpx;
	height: 100%;
	
	height: 100vh;
	
}
.update-tips {
	position: absolute;
	left: 0;
	top: 41px;
	right: 0;
	padding-top: 5px;
	padding-bottom: 5px;
	background-color: #FDDD9B;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	text-align: center;
}
.update-tips-text {
	font-size: 14px;
	color: #ffffff;
}
.refresh {
	width: 750rpx;
	height: 64px;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.refresh-view {
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.refresh-icon {
	width: 30px;
	height: 30px;
	-webkit-transition-duration: .5s;
	        transition-duration: .5s;
	-webkit-transition-property: -webkit-transform;
	transition-property: -webkit-transform;
	transition-property: transform;
	transition-property: transform, -webkit-transform;
	-webkit-transform: rotate(0deg);
	        transform: rotate(0deg);
	-webkit-transform-origin: 15px 15px;
	        transform-origin: 15px 15px;
}
.refresh-icon-active {
	-webkit-transform: rotate(180deg);
	        transform: rotate(180deg);
}
.loading-icon {
	width: 20px;
	height: 20px;
	margin-right: 5px;
	color: #999999;
}
.loading-text {
	margin-left: 2px;
	font-size: 16px;
	color: #999999;
}
.loading-more {
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	padding-top: 10px;
	padding-bottom: 10px;
	text-align: center;
}
.loading-more-text {
	font-size: 28rpx;
	color: #999;
}
#buttons {
	width: 660rpx;
	height: 84rpx;
	line-height: 84rpx;
	margin-top: 10rpx;
	margin-bottom: 100rpx;
	margin-left: 40rpx;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
}
button {
	height: 84rpx;
	font-size: 32rpx;
}
.button {
	padding-left: 20rpx;
	padding-right: 20rpx;
	-webkit-flex: 1;
	-webkit-box-flex: 1;
	        flex: 1;
	height: 84rpx;
}
.box {
	height: 300rpx;
}
.titlebar{
	background-color: #ff9800;
	width: 710rpx;
	position: fixed;
}
.workRadius{
	height: 36rpx;width: 36rpx; border: 3rpx solid #007AFF; border-radius: calc(50%);  display:-webkit-box;  display:-webkit-flex;  display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center; margin: 5rpx 0 5rpx 0;
}
.workSolid{
	background-color: #007AFF;min-height: 170rpx; width: 4rpx;margin-left: 19rpx;
}
.workNoSolid{
	background-color: #969696;min-height: 170rpx; width: 4rpx;margin-left: 19rpx;
}
.hideInput{
	display: none;
}


