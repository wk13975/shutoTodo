.example-title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 32rpx;
	color: #464e52;
	padding: 20rpx 30rpx 20rpx 50rpx;
	margin-top: 6rpx;

	position: relative;
	background-color: #fdfdfd;
	border-bottom: 1px #f5f5f5 solid
}
.example-title__after {
	position: relative;
	color: #031e3c
}
.example-title:after {
	content: '';
	position: absolute;
	left: 30rpx;
	margin: auto;
	top: 0;
	bottom: 0;
	width: 6rpx;
	height: 32rpx;
	background-color: #ccc
}
