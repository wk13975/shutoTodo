<block a:if="{{isShow}}"><view class="{{((('cmd-notice-bar')+' '+(round?'cmd-notice-bar-round':''))+' '+type)}}" style="{{(setNoticeStyle)}}"><block a:if="{{icon!=''}}"><view class="cmd-notice-bar-left"><cmd-icon vue-id="2e7f0238-1" type="{{icon}}" size="16" color="{{setIconColor}}" onVueInit="__l"></cmd-icon></view></block><view class="{{(('cmd-notice-bar-content')+' '+(rows&&!scrollable?'cmd-notice-bar-multi-content':''))}}"><view data-event-opts="{{[['tap',[['$_click',['$event']]]]]}}" class="{{(scrollable?'cmd-notice-bar-content-animate':'')}}" style="{{(setSingleEllStyle)}}" onTap="__e">{{text}}</view></view><block a:if="{{mode!=''}}"><view class="cmd-notice-bar-right"><cmd-icon vue-id="2e7f0238-2" type="{{setModeIcon}}" size="16" color="{{setIconColor}}" data-event-opts="{{[['^click',[['$_mode']]]]}}" onClick="__e" onVueInit="__l"></cmd-icon></view></block></view></block>