
.dialog{
	background: #1a59b7;
	color:#ffffff;
	overflow: hidden;
	z-index: 9999;
	position: fixed;
	padding:5px;
	text-align:center;
	width: 700rpx;
	height: 80%;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	left: 25rpx;
	top:10rpx;
}
input{
	height: 70rpx;
	line-height: 80rpx;
	padding-left: 10rpx;
	padding-right: 10rpx;
	border-radius: 10rpx;
	border: 1rpx solid #d3d3d3;
	background-color: #FFFFFF;
}

