<view class="uni-searchbar"><view class="{{(('uni-searchbar-form')+' '+(show?'':'hide'))}}"><view class="uni-searchbar-form__box" style="{{'border-radius:'+(radius+'rpx')+';'}}"><uni-icons class="icon-search" vue-id="38ebcd10-1" color="#999999" type="search" size="18" data-event-opts="{{[['^click',[['confirm']]]]}}" onClick="__e" onVueInit="__l"></uni-icons><input class="search-input" placeholder="{{placeholder}}" focus="{{show}}" type="text" placeholder-style="color:#cccccc" confirm-type="search" data-event-opts="{{[['confirm',[['confirm',['$event']]]],['input',[['__set_model',['','searchVal','$event',[]]]]]]}}" value="{{searchVal}}" onConfirm="__e" onInput="__e"/><block a:if="{{clearButton==='always'||clearButton==='auto'&&searchVal!==''}}"><uni-icons class="icon-clear" vue-id="38ebcd10-2" color="#999999" type="clear" size="24" data-event-opts="{{[['^click',[['clear']]]]}}" onClick="__e" onVueInit="__l"></uni-icons></block></view><view data-event-opts="{{[['tap',[['searchClick',['$event']]]]]}}" class="uni-searchbar-form__text" style="{{'border-radius:'+(radius+'rpx')+';'}}" onTap="__e"><uni-icons class="icon-search" vue-id="38ebcd10-3" color="#999999" type="search" size="18" onVueInit="__l"></uni-icons><text class="placeholder">{{placeholder}}</text></view><text data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="uni-searchbar-form__cancel" onTap="__e">取消</text></view></view>